# LocalStack Environment Configuration
# Copy this file to .env for docker-compose or export these variables

# Debug mode (0=off, 1=on)
DEBUG=0

# Lambda executor (docker, docker-reuse, or local)
LAMBDA_EXECUTOR=docker

# Services to enable (comma-separated or "all")
# Common services: s3,dynamodb,lambda,ec2,iam,sts,cloudformation,apigateway,sqs,sns,cloudwatch
SERVICES=s3,dynamodb,lambda,ec2,iam,sts

# Enable persistence (0=off, 1=on)
PERSISTENCE=0

# Volume directory for persistence
LOCALSTACK_VOLUME_DIR=./volume
